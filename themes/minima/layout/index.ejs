<div class="row">
  <div class="column">
    <%- partial('partial/projects') %>
  </div>
</div>

<div class="row">
  <div class="column">
    <p class="home fs-18">My latest articles</p>
  </div>
</div>

<% site.posts.sort('date', 'desc' ).limit(theme.max_post).forEach(function(post){ %>
  <div class="post">
    <div class=" post-name">
      <a href="<%- url_for(post.path) %>">
        <% if (post.title=="" ) { %>
          no title
          <% } else { %>
            <%- post.title %>
              <% } %>
      </a>
      <div class="post-description">
        <p class="bottom">
          <%- post.description %>
        </p>
      </div>
      <div class="post-details">
        <p class="bottom">
          <%- full_date(post.date, 'll' ) %> Â· <%- post.readtime %> min read
        </p>
      </div>
    </div>
    <!-- <% if (post.profileimage === true) { %>
      <div>
        <img class="post-image" src="<%-post.path%>profile.jpeg">
      </div>
    <% } %> -->
  </div>
  <% }) %>

